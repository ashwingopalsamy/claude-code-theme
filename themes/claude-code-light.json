  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5F5A52",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords and storage",
      "scope": [
        "keyword",
        "storage",
        "storage.modifier",
        "storage.type",
        "keyword.control",
        "keyword.operator.word"
      ],
      "settings": {
        "foreground": "#B24F33",
        "fontStyle": "bold"
      }
